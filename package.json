  "scripts": {
    "preinstall": "npm i -g eslint@^2.7.0 eslint-plugin-jsx-a11y@^1.0.2 eslint-plugin-react@^5.0.1 eslint-plugin-import eslint-config-airbnb eslint-plugin-babel babel-core babel-cli babel-eslint babel-register rimraf",
    "clean": "rimraf ./build/",
    "build:replace-static-assets": "babel-node ./replace-static-assets.js",
    "build:webpack": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config ./webpack-prod.config.babel.js",
    "build:webpack-windows": "set BABEL_ENV=production && ./node_modules/webpack/bin/webpack.js --config ./webpack-prod.config.babel.js",
    "build": "npm run clean && npm run build:webpack && npm run build:replace-static-assets",
    "build:windows": "npm run clean && npm run build:webpack-windows && npm run build:replace-static-assets",
    "start": "NODE_ENV=development && babel-node ./dev-server.js",
    "start:windows": "set BABEL_ENV=development && babel-node ./dev-server.js",
    "lint": "eslint src"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.4.0",
